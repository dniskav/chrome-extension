{
  "manifest_version": 3,
  "name": "Miles+ Filler",
  "description": "This extension is to fill Miles+ easier",
  "version": "0.1",
  "author": "D-niskav",
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Milles+ Filler",
        "icons": {
          "16": "icons/icon16.png",
          "32": "icons/icon32.png",
          "48": "icons/icon48.png",
          "128": "icons/icon128.png"
        }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/background.js"],
      "matches": [
        "https://websso.t-systems.com/milesplus/prod/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}